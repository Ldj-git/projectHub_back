head
  title Login

body
  H1 Login
  if message != undefined
    p=message
  form(method="post", action="/login/process")
    p ID :
    input(type="text", name="id", placeholder="ID")
    p Password :
    input(type="password", name="pwd", placeholder="Password")
    br
    input(type="submit", value="Login")
  a(href="/register") register